{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/AboutFeatures/index.jsx",
    "components/Hero/index.jsx",
    "components/About/Contact/index.jsx",
    "app/about/AboutSectionTwo.jsx",
    "app/about/AboutSectionThree.jsx",
    "app/about/AboutSectionOne.jsx",
    "app/about/AboutSectionFour.jsx"
, "app/about/AboutTeam.jsx"  ],
  "exclude": ["node_modules"]
}
